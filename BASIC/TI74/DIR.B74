  100 PRINT "PATH:";:ACCEPT AT(7),PATH$
  110 OPEN #1,"100.$"&PATH$,DISPLAY,INPUT
  120 ON BREAK ERROR:ON ERROR 220
  130 IF EOF(1) THEN GOTO 220
  140 INPUT #1,L$,N$,T$
  150 PRINT L$&" "&N$&" "&T$
  160 C$=KEY$:C$=CHR$(ASC(C$) AND NOT 32)
  161 IF C$="D" THEN DELETE "100."&N$
  170 IF C$="R" THEN CLOSE #1:RUN "100."&N$
  180 IF C$<>"N" THEN 140
  190 INPUT "name: ";NN$
  200 OPEN #2,"100":PRINT #2,"rn "&N$&"="&NN$:CLOSE #2
  210 GOTO 130
  220 CLOSE #1
  230 END